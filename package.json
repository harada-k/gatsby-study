{
  "name": "gatsby-study",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-study",
  "author": "harada-k",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "ts-node ./node_modules/.bin/gatsby develop",
    "start": "ts-node ./node_modules/.bin/gatsby develop",
    "build": "ts-node ./node_modules/.bin/gatsby build",
    "serve": "ts-node ./node_modules/.bin/gatsby serve",
    "clean": "ts-node ./node_modules/.bin/gatsby clean",
    "lint": "eslint 'src/**/*.{ts,tsx}' && tsc --noEmit",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' && tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.6.2",
    "gatsby-plugin-graphql-codegen": "^3.0.0",
    "gatsby-plugin-styled-components": "^4.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/styled-components": "^5.1.9",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cli": "^3.7.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1"
  }
}
